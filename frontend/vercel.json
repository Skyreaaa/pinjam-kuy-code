{
  "version": 2,
  "builds": [
    { "src": "be-pinjam-master/server.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "be-pinjam-master/server.js" },
    { "src": "^(?!/api/).*", "dest": "/" }
  ],
  "env": {
    "USE_CLOUDINARY": "true",
    "CI": "false"
  }
}